{
    "name": "zersuxx/slim-skeleton",
    "description": "Slim skeleton app",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">= 7.2",
        "ext-pdo": "*"
        "robmorgan/phinx": "^0.10.6",
        "vlucas/phpdotenv": "^2.5",
        "slim/slim": "^3.10",
        "slim/twig-view": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    }
}
